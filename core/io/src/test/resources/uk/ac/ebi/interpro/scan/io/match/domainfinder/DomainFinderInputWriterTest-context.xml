<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="gene3dParser" class="uk.ac.ebi.interpro.scan.io.match.hmmer3.Hmmer3SearchMatchParser">
        <property name="parserSupport">
            <bean class="uk.ac.ebi.interpro.scan.io.match.hmmer3.Gene3DHmmer3ParserSupport">
                <property name="signatureLibraryRelease" value="Gene3D"/>
                <property name="signatureLibraryName"    value="3.3.0"/>
                <property name="alignmentEncoder">
                    <bean class="uk.ac.ebi.interpro.scan.model.raw.alignment.CigarAlignmentEncoder"/>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="gene3dFile" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg value="data/hmmer3/hmmsearch/2hxsA00.out"/>
    </bean>
   
     <bean id="domainFinderInputFileTest" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg value="data/hmmer3/hmmsearch/df3_input_test.txt"/>
    </bean>

</beans>