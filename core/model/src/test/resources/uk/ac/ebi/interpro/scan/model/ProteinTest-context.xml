<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                           http://www.springframework.org/schema/util 
                           http://www.springframework.org/schema/util/spring-util.xsd">

    <import resource="jpa-context.xml"/>
    <import resource="oxm-context.xml"/>

    <bean id="dao" class="uk.ac.ebi.interpro.scan.genericjpadao.GenericDAOImpl">
        <constructor-arg value="uk.ac.ebi.interpro.scan.model.Protein" />
    </bean>    

    <util:map id="objectXmlMap" value-type="uk.ac.ebi.interpro.scan.model.ObjectXmlPair">

        <!-- TODO: Add xrefs -->
        <entry key="Pfam">
            <bean class="uk.ac.ebi.interpro.scan.model.ObjectXmlPair">
                <constructor-arg>
                    <bean class="uk.ac.ebi.interpro.scan.model.Protein">
                        <constructor-arg value="MDLSALRVEEVQNVINAMQKILECPICLELIKEPVSTKCDHIFCKFCMLKLLNQKKGPSQCPLCKNDI"/>
                        <constructor-arg>
                            <util:set value-type="uk.ac.ebi.interpro.scan.model.FilteredHmmMatch">
                                <bean class="uk.ac.ebi.interpro.scan.model.FilteredHmmMatch">
                                    <constructor-arg>
                                        <bean class="uk.ac.ebi.interpro.scan.model.Signature">
                                            <constructor-arg value="PF02310"/>
                                            <constructor-arg value="B12-binding"/>
                                        </bean>
                                    </constructor-arg>
                                    <constructor-arg value="0.035"/>   <!--Score-->
                                    <constructor-arg value="3.7e-9"/>  <!--E-value-->
                                    <constructor-arg>
                                        <util:set value-type="uk.ac.ebi.interpro.scan.model.HmmLocation">
                                            <bean class="uk.ac.ebi.interpro.scan.model.HmmLocation">
                                                <constructor-arg value="3"/>        <!--Start-->
                                                <constructor-arg value="107"/>      <!--End-->
                                                <constructor-arg value="3.0"/>      <!--Score-->
                                                <constructor-arg value="3.7e-9"/>   <!--E-value-->
                                                <constructor-arg value="1"/>        <!--HMM start-->
                                                <constructor-arg value="104"/>      <!--HMM end-->
                                                <constructor-arg value="N_TERMINAL_COMPLETE"
                                                                 type="uk.ac.ebi.interpro.scan.model.HmmLocation$HmmBounds"/>
                                            </bean>
                                        </util:set>
                                     </constructor-arg>
                                </bean>
                            </util:set>
                        </constructor-arg>
                    </bean>
                </constructor-arg>
                <constructor-arg>
                    <value>
                    <![CDATA[
                        <protein md5="5ab17489095dd2836122eec0e91db82d"
                                 xmlns="http://www.ebi.ac.uk/schema/interpro/scan/model">
                            <sequence>MDLSALRVEEVQNVINAMQKILECPICLELIKEPVSTKCDHIFCKFCMLKLLNQKKGPSQCPLCKNDI</sequence>
                            <matches>
                                <hmm-match score="0.035" evalue="3.7E-9">
                                    <signature ac="PF02310" name="B12-binding" />
                                    <locations>
                                        <hmm-location start="3" end="107" score="3.0" evalue="3.7E-9"
                                                      hmm-start="1" hmm-end="104" hmm-bounds="N_TERMINAL_COMPLETE" />
                                    </locations>
                                </hmm-match>
                            </matches>
                        </protein>
                    ]]>
                    </value>
                </constructor-arg>
            </bean>
        </entry>

        <!--TODO: Add P02700 and OPSD_SHEEP as xrefs-->
        <entry key="PRINTS">
            <bean class="uk.ac.ebi.interpro.scan.model.ObjectXmlPair">
                <constructor-arg>
                    <bean class="uk.ac.ebi.interpro.scan.model.Protein">
                        <constructor-arg value="MNGTEGPNFYVPFSNKTGVVRSPFEAPQYYLAEPWQFSMLAAYMFLLIVLGFPIN"/>
                        <constructor-arg>
                            <util:set value-type="uk.ac.ebi.interpro.scan.model.FilteredFingerPrintsMatch">
                                <bean class="uk.ac.ebi.interpro.scan.model.FilteredFingerPrintsMatch">
                                    <constructor-arg>
                                        <bean class="uk.ac.ebi.interpro.scan.model.Signature">
                                            <constructor-arg value="PR01713"/>
                                            <constructor-arg value="NUCEPIMERASE"/>
                                        </bean>
                                    </constructor-arg>
                                    <constructor-arg value="3.7E-9"/>               <!--E-value-->
                                    <constructor-arg>
                                        <util:set value-type="uk.ac.ebi.interpro.scan.model.FingerPrintsLocation">
                                            <bean class="uk.ac.ebi.interpro.scan.model.FingerPrintsLocation">
                                                <constructor-arg value="220"/>      <!--Start-->
                                                <constructor-arg value="235"/>      <!--End-->
                                                <constructor-arg value="3.7e-9"/>   <!--P-value-->
                                            </bean>
                                            <bean class="uk.ac.ebi.interpro.scan.model.FingerPrintsLocation">
                                                <constructor-arg value="243"/>      <!--Start-->
                                                <constructor-arg value="258"/>      <!--End-->
                                                <constructor-arg value="3.7e-9"/>   <!--P-value-->
                                            </bean>                                            
                                        </util:set>
                                     </constructor-arg>
                                </bean>
                            </util:set>
                        </constructor-arg>
                    </bean>
                </constructor-arg>
                <constructor-arg>
                    <value>
                    <![CDATA[
                       <protein md5="ae2f4d0c928c8017da2df2d5cf15f01"
                                xmlns="http://www.ebi.ac.uk/schema/interpro/scan/model">
                           <sequence>MNGTEGPNFYVPFSNKTGVVRSPFEAPQYYLAEPWQFSMLAAYMFLLIVLGFPIN</sequence>
                           <matches>
                               <fingerprints-match evalue="3.7E-9">
                                   <signature ac="PR01713" name="NUCEPIMERASE"/>
                                   <locations>
                                       <fingerprint-location start="220" end="235" pvalue="3.7E-9"/>
                                       <fingerprint-location start="243" end="258" pvalue="3.7E-9"/>
                                   </locations>
                               </fingerprints-match>
                           </matches>
                       </protein>
                    ]]>
                    </value>
                </constructor-arg>
            </bean>
        </entry>                       

    </util:map>

</beans>