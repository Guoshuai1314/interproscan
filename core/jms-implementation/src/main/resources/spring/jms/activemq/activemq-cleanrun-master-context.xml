<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:amq="http://activemq.apache.org/schema/core"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://activemq.apache.org/schema/core http://activemq.apache.org/schema/core/activemq-core.xsd">

    <import resource="classpath:/spring/spring-properties-context.xml"/>
    <import resource="classpath:/spring/directoryManager/TemporaryDirectoryManager-context.xml"/>

    <bean id="databaseCleaner" class="uk.ac.ebi.interpro.scan.jms.activemq.CleanRunDatabase">
        <property name="installedDatabasePath" value="${i5.h2.database.installed.location}"/>
        <property name="originalDatabasePath" value="${i5.h2.database.original.location}"/>
        <property name="directoryManager" ref="tempDirectoryManager"/>
        <!--<property name="installedDatabasePath" value="support/interpro.h2.db"/>-->
        <!--<property name="originalDatabasePath" value="support/clean_database/interpro.h2.db"/>-->
    </bean>

</beans>
