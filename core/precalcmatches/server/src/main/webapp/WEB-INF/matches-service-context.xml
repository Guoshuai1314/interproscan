<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="matchesService" class="uk.ac.ebi.interpro.scan.precalc.server.service.impl.MatchesServiceImpl"/>

    <bean id="berkeleyMatchDBService"
          class="uk.ac.ebi.interpro.scan.precalc.server.service.impl.BerkeleyMatchDBService">
        <!-- This constructor argument is the path to the Berkeley DB containing all of the matches.-->
        <constructor-arg value="/home/pjones/onion_berkeley"/>
    </bean>

    <bean id="berkeleyMD5Service" class="uk.ac.ebi.interpro.scan.precalc.server.service.impl.BerkeleyMD5DBService">

        <constructor-arg value="/home/pjones/MD5_berkeley"/>
    </bean>

</beans>
