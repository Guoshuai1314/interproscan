<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="binaryRunner" class="uk.ac.ebi.interpro.scan.business.binary.SimpleBinaryRunner">
        <property name="binary"               value="DomainFinder3"/>
        <property name="arguments"            value="-l 5900 -e 0.001"/>
        <property name="deleteTemporaryFiles" value="false"/>
        <property name="commandLineConversation">
            <bean class="uk.ac.ebi.interpro.scan.io.cli.CommandLineConversationImpl"/>
        </property>
    </bean>

    <bean id="rawMatches" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg value="uk/ac/ebi/interpro/scan/business/filter/proteins-raw.txt"/>
    </bean>

    <bean id="filteredMatches" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg value="uk/ac/ebi/interpro/scan/business/filter/proteins-filtered.txt"/>
    </bean>

</beans>
